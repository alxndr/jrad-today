{
  "name": "jrad-today",
  "description": "Code which powers the JRAD fan portal https://JRAD.today",
  "engines": {
    "node": ">=16.13"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.static": "vite build --ssr src/entry.static.tsx",
    "build.types": "tsc --incremental --noEmit",
    "data": "download https://raw.githubusercontent.com/alxndr/almost-dead-net/main/src/data/csv/shows.csv > src/data/shows.csv && download https://raw.githubusercontent.com/alxndr/almost-dead-net/main/src/data/csv/recordings.csv > src/data/recordings.csv",
    "deploy.prod": "export TMPDIR=/private/tmp/dist-$TIMESTAMP && npm run build.static && npm run ssg && cp -r dist $TMPDIR && git co gh-pages && git pull && rm -rf ./* && cp -r $TMPDIR/dist/* . && git add . && git ci -m Deploy\\ $TIMESTAMP && git push && git co main && echo not yet creating the tag deploy-$TIMESTAMP",
    "deploy.prod.gh-pages": "",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "postdeploy.prod": "echo Publish finishing @ $(date '+%F-%T-%Z') â€” started at $TIMESTAMP",
    "predeploy.prod": "export TIMESTAMP=$(date '+%F-%T-%Z') && echo Publish starting @ $TIMESTAMP",
    "preview": "qwik build preview && vite preview --open",
    "ssg": "node server/entry.static",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.9.0",
    "@builder.io/qwik-city": "0.0.111",
    "@types/eslint": "8.4.6",
    "@types/node": "latest",
    "@types/papaparse": "^5.3.5",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "download-cli": "^1.1.1",
    "eslint": "8.23.1",
    "eslint-plugin-qwik": "0.9.0",
    "node-fetch": "3.2.10",
    "papaparse": "^5.3.2",
    "prettier": "2.7.1",
    "typescript": "4.8.3",
    "vite": "3.1.3",
    "vite-tsconfig-paths": "3.5.0"
  }
}
